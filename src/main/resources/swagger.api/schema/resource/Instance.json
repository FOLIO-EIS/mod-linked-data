{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "description": "Instance info",
  "allOf": [
    {
      "$ref": "Topper.json"
    },
    {
      "type": "object",
      "properties": {
        "title": {
          "type": "array",
          "items": {
            "oneOf": [
              {
                "type": "object",
                "title": "InstanceTitleField",
                "properties": {
                  "InstanceTitle": {
                    "type": "object",
                    "$ref": "title/InstanceTitle.json",
                    "x-json-property": "http://bibfra.me/vocab/marc/Title"
                  }
                }
              },
              {
                "type": "object",
                "title": "VariantTitleField",
                "properties": {
                  "VariantTitle": {
                    "type": "object",
                    "$ref": "title/VariantTitle.json",
                    "x-json-property": "http://bibfra.me/vocab/marc/VariantTitle"
                  }
                }
              },
              {
                "type": "object",
                "title": "ParallelTitleField",
                "properties": {
                  "ParallelTitle": {
                    "type": "object",
                    "$ref": "title/ParallelTitle.json",
                    "x-json-property": "http://bibfra.me/vocab/marc/ParallelTitle"
                  }
                }
              }
            ]
          },
          "x-json-property": "http://bibfra.me/vocab/marc/title"
        },
        "production": {
          "type": "array",
          "items": {
            "type": "object",
            "$ref": "ProviderEvent.json"
          },
          "x-json-property": "http://bibfra.me/vocab/marc/production"
        },
        "publication": {
          "type": "array",
          "items": {
            "type": "object",
            "$ref": "ProviderEvent.json"
          },
          "x-json-property": "http://bibfra.me/vocab/marc/publication"
        },
        "distribution": {
          "type": "array",
          "items": {
            "type": "object",
            "$ref": "ProviderEvent.json"
          },
          "x-json-property": "http://bibfra.me/vocab/marc/distribution"
        },
        "manufacture": {
          "type": "array",
          "items": {
            "type": "object",
            "$ref": "ProviderEvent.json"
          },
          "x-json-property": "http://bibfra.me/vocab/marc/manufacture"
        },
        "map": {
          "type": "array",
          "items": {
            "oneOf": [
              {
                "type": "object",
                "title": "LccnField",
                "properties": {
                  "Lccn": {
                    "type": "object",
                    "$ref": "identifier/Lccn.json",
                    "x-json-property": "http://library.link/identifier/LCCN"
                  }
                }
              },
              {
                "type": "object",
                "title": "IsbnField",
                "properties": {
                  "Isbn": {
                    "type": "object",
                    "$ref": "identifier/Isbn.json",
                    "x-json-property": "http://library.link/identifier/ISBN"
                  }
                }
              },
              {
                "type": "object",
                "title": "LocalIdField",
                "properties": {
                  "LocalId": {
                    "type": "object",
                    "$ref": "identifier/LocalId.json",
                    "x-json-property": "http://bibfra.me/vocab/lite/LocalId"
                  }
                }
              },
              {
                "type": "object",
                "title": "EanField",
                "properties": {
                  "Ean": {
                    "type": "object",
                    "$ref": "identifier/Ean.json",
                    "x-json-property": "http://bibfra.me/vocab/identifier/Ean"
                  }
                }
              },
              {
                "type": "object",
                "title": "OtherIdField",
                "properties": {
                  "Identifier": {
                    "type": "object",
                    "$ref": "identifier/OtherId.json",
                    "x-json-property": "http://library.link/identifier/UNKNOWN"
                  }
                }
              }
            ]
          },
          "x-json-property": "http://library.link/vocab/map"
        },
        "supplementaryContent": {
          "type": "array",
          "items": {
            "type": "object",
            "$ref": "SupplementaryContent.json"
          },
          "x-json-property": "http://bibfra.me/vocab/marc/supplementaryContent"
        },
        "accessLocation": {
          "type": "array",
          "items": {
            "type": "object",
            "$ref": "AccessLocation.json"
          },
          "x-json-property": "http://bibfra.me/vocab/marc/accessLocation"
        },
        "media": {
          "type": "array",
          "items": {
            "type": "object",
            "$ref": "Category.json"
          },
          "x-json-property": "http://bibfra.me/vocab/marc/media"
        },
        "carrier": {
          "type": "array",
          "items": {
            "type": "object",
            "$ref": "Category.json"
          },
          "x-json-property": "http://bibfra.me/vocab/marc/carrier"
        },
        "edition": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "x-json-property": "http://bibfra.me/vocab/marc/edition"
        },
        "copyright": {
          "type": "array",
          "items": {
            "type": "object",
            "$ref": "CopyrightEvent.json"
          },
          "x-json-property": "http://bibfra.me/vocab/marc/copyright"
        },
        "extent": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "x-json-property": "http://bibfra.me/vocab/lite/extent"
        },
        "dimensions": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "x-json-property": "http://bibfra.me/vocab/marc/dimensions"
        },
        "projectProvisionDate": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "x-json-property": "http://bibfra.me/vocab/bflc/projectedProvisionDate",
          "description": "Projected publication date (YYMM)"
        },
        "issuance": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "x-json-property": "http://bibfra.me/vocab/marc/issuance"
        },
        "_notes": {
          "type": "array",
          "items": {
            "type": "object",
            "$ref": "../noteDto.json"
          },
          "x-json-property": "_notes"
        },
        "instantiates": {
          "description": "DEPRECATED: to be removed, temporary support of current ui",
          "type": "array",
          "items": {
            "type": "object",
            "$ref": "Work.json"
          },
          "x-json-property": "http://bibfra.me/vocab/lite/instantiates"
        },
        "_workReference": {
          "type": "array",
          "items": {
            "type": "object",
            "$ref": "WorkReference.json"
          }
        },
        "inventoryId": {
          "type": "string",
          "format": "uuid"
        },
        "srsId": {
          "type": "string",
          "format": "uuid"
        }
      }
    }
  ]
}




