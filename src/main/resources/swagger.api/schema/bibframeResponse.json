{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "description": "Get bibframe record response",
  "type": "object",
  "properties": {
    "id": {
      "type": "integer",
      "description": "Bibframe record id"
    },
    "graphName": {
      "type": "string",
      "description": "The name of the graph."
    },
    "graphHash": {
      "type": "integer",
      "description": "The hash value of the graph."
    },
    "slug": {
      "type": "string",
      "description": "The unique text identifier."
    },
    "configuration": {
      "type": "object",
      "description": "The configuration JSON."
    }
  }
}
