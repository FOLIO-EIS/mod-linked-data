{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "description": "Instance info",
  "type": "object",
  "properties": {
    "title": {
      "type": "array",
      "items": {
        "oneOf": [
          {
            "type": "object",
            "title": "InstanceTitleField",
            "properties": {
              "InstanceTitle": {
                "type": "object",
                "$ref": "title/InstanceTitle.json",
                "x-json-property": "http://bibfra.me/vocab/marc/Title"
              }
            }
          },
          {
            "type": "object",
            "title": "VariantTitleField",
            "properties": {
              "VariantTitle": {
                "type": "object",
                "$ref": "title/VariantTitle.json",
                "x-json-property": "http://bibfra.me/vocab/marc/VariantTitle"
              }
            }
          },
          {
            "type": "object",
            "title": "ParallelTitleField",
            "properties": {
              "ParallelTitle": {
                "type": "object",
                "$ref": "title/ParallelTitle.json",
                "x-json-property": "http://bibfra.me/vocab/marc/ParallelTitle"
              }
            }
          }
        ]
      },
      "x-json-property": "http://bibfra.me/vocab/bf/title"
    },
    "provisionActivity": {
      "type": "array",
      "items": {
        "oneOf": [
          {
            "type": "object",
            "title": "PublicationField",
            "properties": {
              "Publication": {
                "type": "object",
                "$ref": "provision/ProvisionActivity.json",
                "x-json-property": "https://bibfra.me/vocab/lite/ProviderEvent"
              }
            }
          },
          {
            "type": "object",
            "title": "DistributionField",
            "properties": {
              "Distribution": {
                "type": "object",
                "$ref": "provision/ProvisionActivity.json",
                "x-json-property": "https://bibfra.me/vocab/lite/DistributionEvent"
              }
            }
          },
          {
            "type": "object",
            "title": "ManufactureField",
            "properties": {
              "Manufacture": {
                "type": "object",
                "$ref": "provision/ProvisionActivity.json",
                "x-json-property": "https://bibfra.me/vocab/lite/ManufactureEvent"
              }
            }
          },
          {
            "type": "object",
            "title": "ProductionField",
            "properties": {
              "Production": {
                "type": "object",
                "$ref": "provision/ProvisionActivity.json",
                "x-json-property": "https://bibfra.me/vocab/lite/ProductionEvent"
              }
            }
          }
        ]
      },
      "x-json-property": "https://bibfra.me/vocab/marc/provisionActivity"
    },
    "identifiedBy": {
      "type": "array",
      "items": {
        "oneOf": [
          {
            "type": "object",
            "title": "LccnField",
            "properties": {
              "Lccn": {
                "type": "object",
                "$ref": "identifier/Lccn.json",
                "x-json-property": "http://library.link/identifier/Lccn"
              }
            }
          },
          {
            "type": "object",
            "title": "IsbnField",
            "properties": {
              "Isbn": {
                "type": "object",
                "$ref": "identifier/Isbn.json",
                "x-json-property": "http://library.link/identifier/Isbn"
              }
            }
          },
          {
            "type": "object",
            "title": "OtherIdentifierField",
            "properties": {
              "Identifier": {
                "type": "object",
                "$ref": "identifier/IdentifierOther.json",
                "x-json-property": "http://bibfra.me/vocab/lite/Identifier"
              }
            }
          },
          {
            "type": "object",
            "title": "LocalIdentifierField",
            "properties": {
              "Local": {
                "type": "object",
                "$ref": "identifier/IdentifierLocal.json",
                "x-json-property": "http://bibfra.me/vocab/lite/LocalId"
              }
            }
          },
          {
            "type": "object",
            "title": "EanField",
            "properties": {
              "Ean": {
                "type": "object",
                "$ref": "identifier/Ean.json",
                "x-json-property": "http://id.loc.gov/ontologies/bibframe/Ean"
              }
            }
          }
        ]
      },
      "x-json-property": "http://bibfra.me/vocab/bf/IdentifiedBy"
    },
    "electronicLocator": {
      "type": "array",
      "items": {
        "type": "object",
        "title": "ElectronicLocatorField",
        "properties": {
          "link": {
            "type": "string",
            "x-json-property": "http://bibfra.me/vocab/lite/link"
          },
          "note": {
            "type": "string",
            "x-json-property": "http://bibfra.me/vocab/lite/note"
          }
        }
      },
      "x-json-property": "https://bibfra.me/vocab/marc/electronicLocator"
    },
    "responsibilityStatement": {
      "type": "array",
      "items": {
        "type": "string"
      },
      "x-json-property": "http://bibfra.me/vocab/marc/statementOfResponsibility",
      "description": "Statement of responsibility"
    },
    "edition": {
      "type": "array",
      "items": {
        "type": "string"
      },
      "x-json-property": "http://bibfra.me/vocab/marc/edition"
    },
    "copyrightDate": {
      "type": "array",
      "items": {
        "type": "string"
      },
      "x-json-property": "http://bibfra.me/vocab/lite/copyrightDate"
    },
    "dimensions": {
      "type": "array",
      "items": {
        "type": "string"
      },
      "x-json-property": "http://bibfra.me/vocab/lite/dimensions"
    },
    "projectProvisionDate": {
      "type": "array",
      "items": {
        "type": "string"
      },
      "x-json-property": "http://bibfra.me/vocab/marc/projectedProvisionDate",
      "description": "Projected publication date (YYMM)"
    },
    "mediaType": {
      "type": "array",
      "items": {
        "type": "string"
      },
      "x-json-property": "http://bibfra.me/vocab/marc/mediaType"
    },
    "carrier": {
      "type": "array",
      "items": {
        "type": "string"
      },
      "x-json-property": "http://bibfra.me/vocab/marc/carrier"
    }
  }
}






