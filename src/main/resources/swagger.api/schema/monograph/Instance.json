{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "description": "Instance info",
  "type": "object",
  "properties": {
    "title": {
      "type": "array",
      "items": {
        "oneOf": [
          {
            "type": "object",
            "title": "InstanceTitleField",
            "properties": {
              "InstanceTitle": {
                "type": "object",
                "$ref": "title/InstanceTitle.json",
                "x-json-property": "http://bibfra.me/vocab/marc/Title"
              }
            }
          },
          {
            "type": "object",
            "title": "VariantTitleField",
            "properties": {
              "VariantTitle": {
                "type": "object",
                "$ref": "title/VariantTitle.json",
                "x-json-property": "http://bibfra.me/vocab/marc/VariantTitle"
              }
            }
          },
          {
            "type": "object",
            "title": "ParallelTitleField",
            "properties": {
              "ParallelTitle": {
                "type": "object",
                "$ref": "title/ParallelTitle.json",
                "x-json-property": "http://bibfra.me/vocab/marc/ParallelTitle"
              }
            }
          }
        ]
      },
      "x-json-property": "http://bibfra.me/vocab/marc/title"
    },
    "production": {
      "type": "array",
      "items": {
        "type": "object",
        "$ref": "provision/ProviderEvent.json"
      },
      "x-json-property": "http://bibfra.me/vocab/marc/production"
    },
    "publication": {
      "type": "array",
      "items": {
        "type": "object",
        "$ref": "provision/ProviderEvent.json"
      },
      "x-json-property": "http://bibfra.me/vocab/marc/publication"
    },
    "distribution": {
      "type": "array",
      "items": {
        "type": "object",
        "$ref": "provision/ProviderEvent.json"
      },
      "x-json-property": "http://bibfra.me/vocab/marc/distribution"
    },
    "manufacture": {
      "type": "array",
      "items": {
        "type": "object",
        "$ref": "provision/ProviderEvent.json"
      },
      "x-json-property": "http://bibfra.me/vocab/marc/manufacture"
    },
    "map": {
      "type": "array",
      "items": {
        "oneOf": [
          {
            "type": "object",
            "title": "LccnField",
            "properties": {
              "Lccn": {
                "type": "object",
                "$ref": "identifier/Lccn.json",
                "x-json-property": "http://library.link/identifier/LCCN"
              }
            }
          },
          {
            "type": "object",
            "title": "IsbnField",
            "properties": {
              "Isbn": {
                "type": "object",
                "$ref": "identifier/Isbn.json",
                "x-json-property": "http://library.link/identifier/ISBN"
              }
            }
          },
          {
            "type": "object",
            "title": "LocalIdField",
            "properties": {
              "LocalId": {
                "type": "object",
                "$ref": "identifier/LocalId.json",
                "x-json-property": "http://bibfra.me/vocab/lite/LocalId"
              }
            }
          },
          {
            "type": "object",
            "title": "EanField",
            "properties": {
              "Ean": {
                "type": "object",
                "$ref": "identifier/Ean.json",
                "x-json-property": "http://bibfra.me/vocab/identifier/Ean"
              }
            }
          },
          {
            "type": "object",
            "title": "OtherIdField",
            "properties": {
              "Identifier": {
                "type": "object",
                "$ref": "identifier/OtherId.json",
                "x-json-property": "http://library.link/identifier/UNKNOWN"
              }
            }
          }
        ]
      },
      "x-json-property": "http://bibfra.me/vocab/lite/map"
    },
    "electronicLocator": {
      "type": "array",
      "items": {
        "type": "object",
        "title": "ElectronicLocatorField",
        "properties": {
          "link": {
            "type": "array",
            "items": {
              "type": "string"
            },
            "x-json-property": "http://bibfra.me/vocab/lite/link"
          },
          "note": {
            "type": "array",
            "items": {
              "type": "string"
            },
            "x-json-property": "http://bibfra.me/vocab/lite/note"
          }
        }
      },
      "x-json-property": "https://bibfra.me/vocab/marc/electronicLocator"
    },
    "responsibilityStatement": {
      "type": "array",
      "items": {
        "type": "string"
      },
      "x-json-property": "http://bibfra.me/vocab/marc/statementOfResponsibility",
      "description": "Statement of responsibility"
    },
    "edition": {
      "type": "array",
      "items": {
        "type": "string"
      },
      "x-json-property": "http://bibfra.me/vocab/marc/edition"
    },
    "copyrightDate": {
      "type": "array",
      "items": {
        "type": "string"
      },
      "x-json-property": "http://bibfra.me/vocab/lite/copyrightDate"
    },
    "dimensions": {
      "type": "array",
      "items": {
        "type": "string"
      },
      "x-json-property": "http://bibfra.me/vocab/marc/dimensions"
    },
    "projectProvisionDate": {
      "type": "array",
      "items": {
        "type": "string"
      },
      "x-json-property": "http://bibfra.me/vocab/bflc/projectedProvisionDate",
      "description": "Projected publication date (YYMM)"
    },
    "mediaType": {
      "type": "array",
      "items": {
        "type": "string"
      },
      "x-json-property": "http://bibfra.me/vocab/marc/mediaType"
    },
    "carrier": {
      "type": "array",
      "items": {
        "type": "string"
      },
      "x-json-property": "http://bibfra.me/vocab/marc/carrier"
    },
    "issuance": {
      "type": "array",
      "items": {
        "type": "string"
      },
      "x-json-property": "http://bibfra.me/vocab/marc/issuance"
    }
  }
}






