{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "description": "Parallel Title Info",
  "allOf": [
    {
      "$ref": "BaseTitle.json"
    },
    {
      "type": "object",
      "properties": {
        "note": {
          "type": "array",
          "items": {
            "$ref": "../property.json"
          },
          "x-json-property": "http://id.loc.gov/ontologies/bibframe/note"
        },
        "date": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "x-json-property": "http://id.loc.gov/ontologies/bibframe/date"
        },
        "subtitle": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "x-json-property": "http://id.loc.gov/ontologies/bibframe/subtitle"
        }
      }
    }
  ]
}
